<extension implements="::fwServices::registry::AppConfig">
        <id>manualRegistrationEditorConfig</id>
        <type>parameters</type>
        <config>
        

    <object type="::fwData::Composite">

        <!-- MAIN FRAME -->
        <service uid="registrationEditorFrame" type="::fwGui::IFrameSrv" impl="::gui::frame::DefaultFrame" autoConnect="no">
            <window onclose="notify" />
            <gui>
                <frame>
                    <name>Manual Rgistration Editor</name>
                    <icon>Bundles/PoC03MeshManualRegistration_0-1/tuto.ico</icon>
                    <minSize width="400" height="600" />
                    <style mode="STAY_ON_TOP" />
                </frame>
            </gui>
            <registry>
                <view sid="listOrganTransformationEditor" />
            </registry>
        </service>

        <item key="compositeVisu">
            <object uid="compositeVisu" src="ref" type="::fwData::Composite">

                <service uid="managerForManualRegistEditor" impl="::ctrlSelection::manager::SwapperSrv" type="::ctrlSelection::IManagerSrv" autoConnect="yes">
                    <mode type="dummy" />
                    <config>
                        <object id="acquisition" type="::fwData::Acquisition">
                            <service uid="listOrganTransformationEditor" impl="::uiAcquisition::OrganTransformationEditor" type="::gui::editor::IEditor" autoConnect="yes">
                                <TMSUid>meshDB</TMSUid>
                            </service>
                        </object>
                    </config>
                </service>
            </object>
        </item>

        <!-- START AND STOP SERVICES -->
        <start uid="registrationEditorFrame" />
        <start uid="managerForManualRegistEditor" />

    </object>


        </config>
    </extension>
