<extension implements="::fwServices::registry::AppConfig">
        <id>manualRegistrationEditorConfig</id>
        <type>standard</type>
        <config>
        

    <object type="::fwData::Composite">

        <!-- MAIN FRAME -->
        <service uid="registrationEditorFrame" type="::fwGui::IFrameSrv" implementation="::gui::frame::DefaultFrame" autoComChannel="no">
            <window onclose="notify" />
            <gui>
                <frame>
                    <name>Manual Rgistration Editor</name>
                    <icon>Bundles/PoC03MeshManualRegistration_0-1/tuto.ico</icon>
                    <minSize width="400" height="600" />
                    <style mode="STAY_ON_TOP" />
                </frame>
            </gui>
            <registry>
                <view sid="listOrganTransformationEditor" />
            </registry>
        </service>

        <item key="myCompositeVisu">
            <object uid="myCompositeVisu" src="ref" type="::fwData::Composite">
            
                <service uid="myManagerForManualRegistEditorComChannel" implementation="::fwServices::ComChannelService" type="::fwServices::ICommunication" autoComChannel="no">
                    <target>myManagerForManualRegistEditor</target>
                </service>
                
                <service uid="myManagerForManualRegistEditor" implementation="::ctrlSelection::manager::SwapperSrv" type="::ctrlSelection::IManagerSrv" autoComChannel="no">
                    <mode type="dummy" />
                    <config>
                        <object id="myAcquisition" type="::fwData::Acquisition">
                            <service uid="listOrganTransformationEditor" implementation="::uiAcquisition::OrganTransformationEditor" type="::gui::editor::IEditor" autoComChannel="yes">
                                <TMSUid>MyMeshDB</TMSUid>
                            </service>
                        </object>
                    </config>
                </service>
            </object>
        </item>

        <!-- START AND STOP SERVICES -->
        <start uid="registrationEditorFrame" />
        <start uid="myManagerForManualRegistEditor" />
        <start uid="myManagerForManualRegistEditorComChannel" />

    </object>


        </config>
    </extension>
