<plugin id="uiPatientDB" class="::uiPatientDB::Plugin">
    <library name="uiPatientDB"/>
    
    <requirement id="dataReg"/>
    <requirement id="servicesReg"/>
    <requirement id="gui"/>
    <requirement id="io"/>
    
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::CloneImage</service>
        <object>::fwData::PatientDB</object>
    </extension>
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::AddPatientDB</service>
        <object>::fwData::PatientDB</object>
    </extension>
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::ExportPatientDB</service>
        <object>::fwData::PatientDB</object>
    </extension>
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::ExportPatient</service>
        <object>::fwData::PatientDB</object>
    </extension>
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::ErasePatientDB</service>
        <object>::fwData::PatientDB</object>
    </extension>
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::ErasePatient</service>
        <object>::fwData::PatientDB</object>
    </extension>
    
    <extension implements="::fwServices::registry::ServiceFactory">
        <type>::fwGui::IActionSrv</type>
        <service>::uiPatientDB::action::ExportImage</service>
        <object>::fwData::PatientDB</object>
    </extension>
        
</plugin>
