<plugin class="::guiQt::Plugin" id="guiQt">

    <library name="guiQt"/>

    <requirement id="dataReg"/>
    <requirement id="servicesReg"/>

    <point id="::guiQt::editor::IEditor">
        <implements>::fwServices::IService</implements>
    </point>
    
    <point id="::guiQt::aspect::IAspect">
        <implements>::fwServices::IService</implements>
    </point>
    
    <point id="::guiQt::aspect::IMenuBar">
        <implements>::fwServices::IService</implements>
    </point>

    <point id="::guiQt::action::IAction">
        <implements>::fwServices::IService</implements>
    </point>
    
    <point id="::guiQt::view::IView">
        <implements>::fwServices::IService</implements>
    </point>

    <point id="::guiQt::action::QuitAction">
        <implements>::guiQt::action::IAction</implements>
        <implements>::fwTools::Object</implements>
    </point>
    
    <point id="::guiQt::action::StarterActionService">
        <implements>::guiQt::action::IAction</implements>
        <implements>::fwTools::Object</implements>
    </point>

    <point id="::guiQt::action::FullScreenActionService">
        <implements>::guiQt::action::IAction</implements>
        <implements>::fwTools::Object</implements>
    </point>
    
    <point id="::guiQt::aspect::DefaultMenuBar">
        <implements>::gui::aspect::IMenu</implements>
        <implements>::fwTools::Object</implements>
    </point>
    
    <point id="::guiQt::aspect::DefaultAspect">
        <implements>::guiQt::aspect::IAspect</implements>
        <implements>::fwTools::Object</implements>
    </point>
    
    <point id="::guiQt::view::DefaultView">
        <implements>::guiQt::view::IView</implements>
        <implements>::fwTools::Object</implements>
    </point>
  
</plugin>
